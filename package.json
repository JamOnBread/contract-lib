{
  "name": "@jamonbread/sdk",
  "description": "JoB smart contract manipulation",
  "version": "1.1.0",
  "license": "Apache-2.0",
  "type": "module",
  "engines": {
    "node": ">=18.15.0"
  },
  "files": [
    "./dist",
    "./src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/sdk.cjs",
      "default": "./dist/sdk.js"
    }
  },
  "typesVersions": {
    ">=5": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "dependencies": {
    "lucid-cardano": "^0.10.7",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "~18.15.0",
    "@vitest/coverage-v8": "^1.2.2",
    "typescript": "^5.1.6",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "scripts": {
    "clean": "rm -r dist/*",
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "test:dev": "vitest",
    "test": "vitest run",
    "prepare": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamOnBread/contract-lib.git"
  }
}